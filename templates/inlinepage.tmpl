<TMPL_IF HTML5><article class="inlinepage"><TMPL_ELSE><div class="inlinepage"></TMPL_IF>
<TMPL_IF HTML5><section class="inlineheader"><TMPL_ELSE><div class="inlineheader"></TMPL_IF>

<!--  date -->
<TMPL_IF NAME="DAY">
<div class="blogDate">
<span class="dow"><TMPL_VAR DAY></span>
<span class="day"><TMPL_VAR DATE></span>
<span class="month"><TMPL_VAR MONTH> <br /></span>
<span class="year"><TMPL_VAR YEAR></span>
</div><!-- blogDate -->
</TMPL_IF>
<!-- story -->
<div class="story">
<TMPL_IF HTML5><header class="headers"><TMPL_ELSE><div class="headers"></TMPL_IF>
<TMPL_IF NAME="PERMALINK">
<a href="<TMPL_VAR PERMALINK>"><TMPL_VAR TITLE></a>
<br />
<div class="story-links">
<TMPL_IF NAME="TAGS">
<div class="taglink">
<span class="left">[</span><ul><TMPL_LOOP NAME="TAGS"><li class="tag"><TMPL_VAR NAME=LINK>&nbsp;::&nbsp;</li></TMPL_LOOP></ul><span class="right">]</span>
</div> <!-- taglink -->
</TMPL_IF>
</div><!-- story-links -->
<TMPL_ELSE>
<a href="<TMPL_VAR PAGEURL>"><TMPL_VAR TITLE></a>
<br />
<TMPL_IF NAME="TAGS">
<div class="taglink">
<span class="left">[</span><ul><TMPL_LOOP NAME="TAGS"><li class="tag"><TMPL_VAR NAME=LINK>&nbsp;::&nbsp;</li></TMPL_LOOP></ul><span class="right">]</span>
</div> <!-- taglink -->
</TMPL_IF>
<p class="postedon">
<span class="pagedate">Posted <TMPL_VAR CTIME></span>
</p>
<TMPL_IF COPYRIGHT>
<div class="pagecopyright">
<TMPL_VAR COPYRIGHT>
</div>
</TMPL_IF>
<TMPL_IF LICENSE>
<div class="pagelicense">
<p>License: <TMPL_VAR LICENSE>
</div><!-- pagelicense -->
</TMPL_IF>

</TMPL_IF>
<TMPL_IF HTML5></headers><TMPL_ELSE></div></TMPL_IF> <!-- headers -->
<TMPL_IF HTML5></section><TMPL_ELSE></div></TMPL_IF><!--inlineheader-->

<TMPL_IF HTML5><section class="inlinecontent"><TMPL_ELSE><div class="inlinecontent"></TMPL_IF>
<div class="story-body">
<TMPL_VAR CONTENT>
</div>  <!-- story-body -->
<TMPL_IF NAME="AUTHOR">
<div class="postedby">
<TMPL_IF NAME="AUTHORURL">
<p class="postedby">
<a href="<TMPL_VAR NAME=AUTHORURL>">
<span class="author"><TMPL_VAR NAME=AUTHOR></span>
</a>
</p>
<TMPL_ELSE>
<p class="postedby">
<span class="author"><TMPL_VAR NAME=AUTHOR></span>
</p>
</TMPL_IF>
</div> <!-- postedby -->
</TMPL_IF>
<TMPL_IF HTML5></section><TMPL_ELSE></div></TMPL_IF>
<TMPL_IF HTML5><footer class="inlinefooter"><TMPL_ELSE><div class="inlinefooter"></TMPL_IF>
<TMPL_IF NAME="HAVE_ACTIONS">
<span class="actions">
<ul>
<TMPL_IF NAME="EDITURL">
<li><a href="<TMPL_VAR EDITURL>"><img src="/media/icons/editor.png" alt="Edit this page" /></a></li>
</TMPL_IF>
<TMPL_IF NAME="COMMENTSLINK">
<li><TMPL_VAR COMMENTSLINK></li>
<TMPL_ELSE>
<TMPL_IF NAME="DISCUSSIONLINK">
<li><TMPL_VAR DISCUSSIONLINK></li>
</TMPL_IF>
</TMPL_IF>
</ul>
</span><!--.actions-->
</TMPL_IF>
<TMPL_IF HTML5></footer><TMPL_ELSE></div></TMPL_IF>

</div><!-- story -->

<TMPL_IF HTML5></article><TMPL_ELSE></div></TMPL_IF> <!-- inlinepage -->

